# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/ai_interview_assistant"

# Redis Configuration
REDIS_URL="redis://localhost:6379"
REDIS_MAX_RETRIES=3
REDIS_LAZY_CONNECT=true

# Cache Configuration
CACHE_DEFAULT_TTL=3600
CACHE_MAX_SIZE=50000
CACHE_ENABLE_COMPRESSION=true
CACHE_ENABLE_METRICS=true
CACHE_CLEANUP_INTERVAL=300000

# API Keys
OPENAI_API_KEY="your_openai_api_key_here"
GOOGLE_CLOUD_PROJECT_ID="your_google_cloud_project_id"
GOOGLE_APPLICATION_CREDENTIALS="path/to/service-account-key.json"

# Authentication
JWT_SECRET="your_jwt_secret_here"
JWT_EXPIRES_IN="7d"

# External Integrations
LINKEDIN_CLIENT_ID="your_linkedin_client_id"
LINKEDIN_CLIENT_SECRET="your_linkedin_client_secret"

# Application Configuration
NODE_ENV="development"
PORT=3001
FRONTEND_URL="http://localhost:3000"

# Performance Monitoring
PERFORMANCE_MONITORING_ENABLED=true
METRICS_RETENTION_HOURS=24
CACHE_HIT_RATE_THRESHOLD=80
CACHE_LATENCY_THRESHOLD=100
MEMORY_USAGE_THRESHOLD=512
ERROR_RATE_THRESHOLD=5

# Rate Limiting
RATE_LIMITING_ENABLED=true
RATE_LIMIT_API_WINDOW=60000
RATE_LIMIT_API_MAX=100
RATE_LIMIT_AUTH_WINDOW=300000
RATE_LIMIT_AUTH_MAX=5
RATE_LIMIT_TRANSCRIPTION_WINDOW=60000
RATE_LIMIT_TRANSCRIPTION_MAX=20
RATE_LIMIT_RESPONSE_WINDOW=60000
RATE_LIMIT_RESPONSE_MAX=30
RATE_LIMIT_USER_WINDOW=60000
RATE_LIMIT_USER_MAX=200

# Precomputation
PRECOMPUTATION_ENABLED=true
PRECOMPUTE_RESPONSES_SCHEDULE="0 2 * * *"
PRECOMPUTE_CONTEXT_SCHEDULE="0 3 * * *"
PRECOMPUTE_TRANSCRIPTION_SCHEDULE="0 4 * * 0"

# Database Optimization
DB_CONNECTION_LIMIT=20
DB_POOL_TIMEOUT=60000
DB_IDLE_TIMEOUT=600000
DB_WORK_MEM=4MB
DB_RANDOM_PAGE_COST=1.1
DB_MAX_PARALLEL_WORKERS=2
DB_CHECKPOINT_COMPLETION_TARGET=0.9
DB_EFFECTIVE_CACHE_SIZE=1GB

# Cache Warming
CACHE_WARMING_ENABLED=true
CACHE_WARMING_BATCH_SIZE=100
CACHE_WARMING_CONCURRENCY=5

# File Upload
MAX_FILE_SIZE=10485760
UPLOAD_DEST="./uploads"

# Monitoring
LOG_LEVEL="info"
ENABLE_METRICS=true# 
Security and Privacy Configuration
ENCRYPTION_MASTER_KEY="your-32-character-master-encryption-key-here"
DATA_EXPORT_DIR="./data-exports"
AUDIT_LOG_RETENTION_DAYS=365
FRAUD_DETECTION_ENABLED=true
AUTO_CLEANUP_ENABLED=true

# Privacy Policy Version
PRIVACY_POLICY_VERSION="1.0.0"
TERMS_OF_SERVICE_VERSION="1.0.0"

# Data Retention Defaults (in days)
DEFAULT_AUDIO_RETENTION_DAYS=30
DEFAULT_TRANSCRIPTION_RETENTION_DAYS=90
DEFAULT_SESSION_RETENTION_DAYS=365
DEFAULT_ANALYTICS_RETENTION_DAYS=730

# Fraud Detection Thresholds
FRAUD_RISK_LOW_THRESHOLD=30
FRAUD_RISK_MEDIUM_THRESHOLD=60
FRAUD_RISK_HIGH_THRESHOLD=80
FRAUD_RISK_CRITICAL_THRESHOLD=95

# Usage Limits (Free Tier)
FREE_TIER_SESSIONS_PER_DAY=5
FREE_TIER_AUDIO_MINUTES_PER_DAY=60
FREE_TIER_API_CALLS_PER_HOUR=100

# Usage Limits (Pro Tier)
PRO_TIER_SESSIONS_PER_DAY=50
PRO_TIER_AUDIO_MINUTES_PER_DAY=600
PRO_TIER_API_CALLS_PER_HOUR=1000

# Usage Limits (Enterprise Tier)
ENTERPRISE_TIER_SESSIONS_PER_DAY=500
ENTERPRISE_TIER_AUDIO_MINUTES_PER_DAY=6000
ENTERPRISE_TIER_API_CALLS_PER_HOUR=10000

# GDPR Compliance
GDPR_EXPORT_EXPIRY_DAYS=30
GDPR_DELETION_GRACE_PERIOD_DAYS=7
GDPR_DATA_PROCESSOR_EMAIL="privacy@example.com"
GDPR_DATA_PROCESSOR_ADDRESS="Data Protection Officer, Company Address"

# Consent Management
CONSENT_COOKIE_DURATION_DAYS=365
CONSENT_BANNER_ENABLED=true
CONSENT_STRICT_MODE=false
# 
External Integrations - Additional APIs

# Google Calendar API
GOOGLE_CLIENT_ID="your_google_client_id"
GOOGLE_CLIENT_SECRET="your_google_client_secret"
GOOGLE_REDIRECT_URI="http://localhost:3000/api/integrations/calendar/google/callback"

# Microsoft Outlook API
OUTLOOK_CLIENT_ID="your_outlook_client_id"
OUTLOOK_CLIENT_SECRET="your_outlook_client_secret"
OUTLOOK_REDIRECT_URI="http://localhost:3000/api/integrations/calendar/outlook/callback"

# Zoom API
ZOOM_CLIENT_ID="your_zoom_client_id"
ZOOM_CLIENT_SECRET="your_zoom_client_secret"
ZOOM_REDIRECT_URI="http://localhost:3000/api/integrations/video-conferencing/zoom/callback"

# Microsoft Teams API
TEAMS_CLIENT_ID="your_teams_client_id"
TEAMS_CLIENT_SECRET="your_teams_client_secret"
TEAMS_REDIRECT_URI="http://localhost:3000/api/integrations/video-conferencing/teams/callback"

# LinkedIn API (Enhanced)
LINKEDIN_REDIRECT_URI="http://localhost:3000/api/integrations/linkedin/callback"

# Data Export Configuration
EXPORT_DIRECTORY="./exports"
EXPORT_MAX_FILE_SIZE="100MB"
EXPORT_RETENTION_HOURS=24
EXPORT_CLEANUP_SCHEDULE="0 */6 * * *"

# Webhook Configuration
WEBHOOK_MAX_RETRIES=3
WEBHOOK_RETRY_DELAYS="1000,5000,15000"
WEBHOOK_TIMEOUT=30000
WEBHOOK_MAX_FAILURES=10
WEBHOOK_CLEANUP_SCHEDULE="0 1 * * *"